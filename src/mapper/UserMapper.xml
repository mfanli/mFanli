<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="User">
    <select id="findAll" resultType="dot.Person">
         SELECT * FROM  person.Person
    </select>

    <insert id="AddBlog" parameterType="dot.Blog">
         INSERT INTO mfanli.blog (subscriberId,category,blogTitle,blog) VALUES (#{subscriberId},#{category},#{blogTitle},#{blog})
    </insert>
</mapper>